      { "pointId": "p3", "position": { "x": 80, "y": 50 }, "movable": false, "radius": 0, "layerIds": ["0"] },
      { "pointId": "p4", "position": { "x": 50, "y": 20 }, "movable": false, "radius": 0, "layerIds": ["0"] },
